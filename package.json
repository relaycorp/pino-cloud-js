{
  "name": "@relaycorp/pino-cloud",
  "version": "1.0.0",
  "author": {
    "email": "no-reply@relaycorp.tech",
    "name": "Relaycorp, Inc.",
    "url": "https://relaycorp.tech/"
  },
  "description": "Pino integration for cloud logging services like Google Cloud Logging",
  "main": "build/main/index.js",
  "typings": "build/main/index.d.ts",
  "module": "build/module/index.js",
  "repository": "https://github.com/relaycorp/pino-cloud-js",
  "homepage": "https://docs.relaycorp.tech/pino-cloud-js/",
  "license": "MIT",
  "keywords": [
    "pino",
    "stackdriver",
    "gcp-cloud-logging",
    "gcp-error-reporting"
  ],
  "scripts": {
    "build": "run-s clean && run-p build:*",
    "build:main": "tsc -p tsconfig.json",
    "build:module": "tsc -p tsconfig.module.json",
    "fix": "run-s fix:*",
    "fix:prettier": "prettier \"src/**/*.ts\" --write",
    "fix:tslint": "tslint --fix --project .",
    "static-checks": "run-p static-checks:*",
    "static-checks:lint": "tslint --project .",
    "static-checks:prettier": "prettier \"src/**/*.ts\" --list-different",
    "test": "jest --coverage",
    "test:ci:unit": "run-s build test:ci:unit:jest",
    "test:ci:unit:jest": "jest --config jest.config.ci.js --coverage",
    "test:integration": "exit 0",
    "doc-api": "typedoc src/index.ts --out build/docs",
    "clean": "del-cli build test"
  },
  "engines": {
    "node": ">=12"
  },
  "dependencies": {
    "pino": "^6.13.0"
  },
  "devDependencies": {
    "@relaycorp/shared-config": "^1.5.3",
    "@types/jest": "^27.0.3",
    "@types/pino": "^6.3.9",
    "del-cli": "^4.0.1",
    "jest": "^24.9.0",
    "jest-extended": "^0.11.5",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.2",
    "ts-jest": "^24.3.0",
    "ts-node": "^10.4.0",
    "tslint": "^5.20.1",
    "typedoc": "^0.21.5",
    "typescript": "^4.3.5"
  },
  "peerDependencies": {
    "pino": "< 7.0.0"
  },
  "prettier": "@relaycorp/shared-config/.prettierrc.json",
  "publishConfig": {
    "access": "public",
    "tag": "latest"
  }
}
